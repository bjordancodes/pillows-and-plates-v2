<script>
function myMap() {

//centers our map in dallas
var mapProp= {
  center:new google.maps.LatLng(32.7762719,-96.7968559),
  zoom:15,
};

var map = new google.maps.Map(document.getElementById("googleMap"),mapProp);
var ministries_Dallas = {lat: 32.7790938, lng: -96.7757069};
//var image = '~/Pictures/PlatesPin.png';
//var marker = new google.maps.Marker({position: ministries_Dallas, map: map, icon: image});
var marker = new google.maps.Marker({position: ministries_Dallas, map: map});
}
</script>

